class Solution {
    public boolean isPalindrome(ListNode head) {
        if(head == null || head.next == null){
            return true;
        }

        ListNode temp = head;
        ListNode temp2 = head;
        Stack<Integer> st = new Stack<>();

        while(temp2 != null && temp2.next != null){
            st.push(temp.val);
            temp = temp.next;
            temp2 = temp2.next.next;       
        }
        if(temp2 != null){
            temp = temp.next;
        }
        while(temp != null){
            int top = st.peek();
            st.pop();
            if(top != temp.val ){
                return false;
            }
            temp=temp.next;
        }

        if(!st.isEmpty()){
            return false;
        }
        return true;
    }
}
